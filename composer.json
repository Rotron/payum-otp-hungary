{
    "name": "konekt/payum-otp",
    "type": "library",
    "description": "Custom Payum gateway for hungarian OTP Bank.",
    "keywords": ["payment", "otp", "payum"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lajos Fazakas"
        }
    ],
    "require": {
        "payum/core": "^1.1.3"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Konekt\\PayumOtp\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "spec\\Konekt\\PayumOtp\\": "spec/" }
    }
}
